<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_content"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:background="@mipmap/water"
    tools:context="amu.haimanot.censussystem4arbaminch.MainActivity"
    tools:layout_editor_absoluteY="25dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/textView23"
        android:layout_width="103dp"
        android:layout_height="27dp"
        android:text="Enum Area:"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/lblSa"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/lblSa"
        android:layout_width="111dp"
        android:layout_height="23dp"
        android:text="Station Area:"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:layout_marginTop="75dp"
        app:layout_constraintTop_toBottomOf="@+id/etKebele"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="87dp"
        android:layout_height="26dp"
        android:text="Town:"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:layout_marginTop="36dp"
        app:layout_constraintTop_toBottomOf="@+id/textView19"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="87dp"
        android:layout_height="26dp"
        android:text="Wereda:"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:layout_marginTop="26dp"
        app:layout_constraintTop_toBottomOf="@+id/textView18"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="87dp"
        android:layout_height="26dp"
        android:text="Zone:"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/textView17"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="208dp"
        android:layout_height="28dp"
        android:text="Area Identification"
        android:textAppearance="@android:style/TextAppearance.Material.Large"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="35dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.144" />

    <Spinner
        android:id="@+id/spinRegion"
        android:layout_width="228dp"
        android:layout_height="26dp"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="88dp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="87dp"
        android:layout_height="26dp"
        android:text="Region:"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <EditText
        android:id="@+id/etKebele"
        android:layout_width="116dp"
        android:layout_height="38dp"
        android:ems="10"
        android:hint="Kebele"
        android:inputType="number"
        android:layout_marginTop="28dp"
        app:layout_constraintTop_toBottomOf="@+id/textView20"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <EditText
        android:id="@+id/etSubcity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="SubCity"
        android:inputType="textPersonName"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/spinTown"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/btnCancel"
        android:layout_width="112dp"
        android:layout_height="48dp"
        android:text="Cancel"
        android:layout_marginLeft="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />
    <Button
        android:id="@+id/btnNext"
        android:layout_width="106dp"
        android:layout_height="46dp"
        android:text="Next"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        android:layout_marginRight="31dp"
        app:layout_constraintRight_toRightOf="parent" />

    <AutoCompleteTextView
        android:id="@+id/spinZone"
        android:layout_width="220dp"
        android:layout_height="47dp"
        android:layout_marginTop="9dp"
        app:layout_constraintTop_toBottomOf="@+id/spinRegion"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent" />

    <AutoCompleteTextView
        android:id="@+id/spinWereda"
        android:layout_width="224dp"
        android:layout_height="41dp"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/spinZone"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent" />

    <AutoCompleteTextView
        android:id="@+id/spinTown"
        android:layout_width="224dp"
        android:layout_height="39dp"
        android:layout_marginTop="21dp"
        app:layout_constraintTop_toBottomOf="@+id/spinWereda"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent" />

    <EditText
        android:id="@+id/etStationArea"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Station Area"
        android:inputType="textPersonName"
        android:layout_marginTop="62dp"
        app:layout_constraintTop_toBottomOf="@+id/etSubcity"
        android:layout_marginRight="29dp"
        app:layout_constraintRight_toRightOf="parent" />

    <EditText
        android:id="@+id/etEnumArea"
        android:layout_width="222dp"
        android:layout_height="48dp"
        android:ems="10"
        android:hint="Enumeration Area"
        android:inputType="textPersonName"
        android:layout_marginRight="33dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/etStationArea" />

</android.support.constraint.ConstraintLayout>